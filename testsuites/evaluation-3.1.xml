<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="ICSE 2015 - Evaluation 3.1 - Exhaustive exploration of state-transitions"
	verbose="1" annotations="JDK">
	<test name="Exploration of the 0 - 10 state-space">
		<parameter name="lowerBound" value="10" />
		<parameter name="upperBound" value="50" />

		<parameter name="minSize" value="0" />
		<parameter name="maxSize" value="10" />

		<parameter name="minPeriodBetweenExecs" value="0" />
		<parameter name="resizeUpStabilizationDelay" value="0" />
		<parameter name="resizeDownStabilizationDelay" value="0" />

		<classes>
			<class name="at.ac.testing.brooklyn.evaluation3.AutomationTest"/>
		</classes>
	</test>
</suite>
